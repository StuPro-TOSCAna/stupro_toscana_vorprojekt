#!/bin/bash  
# requires: $ENVIRONMENTSETUPWEB_CHECKENV - checkenv script
# requires: $ENVIRONMENTSETUPWEB_SETVALUE - setvalue script
# requires: $ENVIRONMENTSETUPWEB_SETSTUB - setstub script

./$ENVIRONMENTSETUPWEB_CHECKENV "$ENVIRONMENTSETUPWEB_CHECKENV" "$ENVIRONMENTSETUPWEB_SETVALUE" "$ENVIRONMENTSETUPWEB_SETSTUB"
[ $? -eq 0 ] || exit 1

# install util scripts
mv $ENVIRONMENTSETUPWEB_SETSTUB /usr/local/bin
mv $ENVIRONMENTSETUPWEB_SETVALUE /usr/local/bin
mv $ENVIRONMENTSETUPWEB_CHECKENV  /usr/local/bin

